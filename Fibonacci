class Solution {
    public int fib(int n) {
        if(n==0)
        return 0;
        if(n==1)
        return 1;
        int t1=1;
        int t2=0;
        int curr=0;
        for(int i=2;i<=n;i++){
            curr=t1+t2;
            t2=t1;
            t1=curr;
        }
        return curr;
        
        
    }
}
